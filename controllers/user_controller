const UserModel = require("../db/user_model");
const bcrypt = require("bcrypt");

const createNewUser = async (req, res) => {
	UserModel.create(req.body, (err, user) => {
		if (err) {
			res.status(422).send({ error: err.message });
		} else {
            res.status(201);
		}
	});
};

module.exports = {
	createNewUser,
};
